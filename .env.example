# Cuepernova Environment Configuration
# Copy this file to .env and customize for your environment

# Server Configuration
NODE_ENV=development
HTTP_PORT=8080
HTTPS_PORT=8443

# OSC Configuration
OSC_PORT=57121
OSC_HOST=0.0.0.0

# SSL Configuration
SSL_KEY_PATH=certs/key.pem
SSL_CERT_PATH=certs/cert.pem

# Logging
LOG_LEVEL=debug

# Performance
MAX_CONNECTIONS=100
CONNECTION_TIMEOUT=30000