  },
  "bugs": {
    "url": "https://github.com/yourusername/cuepernova/issues"
  },
  "dependencies": {
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "osc": "^2.4.4",
    "pug": "^3.0.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
