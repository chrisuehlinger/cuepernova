{
  "name": "cuepernova",
  "version": "0.0.1",
  "description": "Open source theater projection control system",
  "type": "module",
  "main": "dist/server/app.js",
  "types": "dist/types/index.d.ts",
  "bin": {
    "cuepernova": "dist/cli.js"
  },
  "files": [
    "dist/",
    "static/",
    "templates/",
    "views/"
  ],
  "scripts": {
