{
  "name": "cuepernova",
  "version": "0.0.1",
  "description": "Open source theater projection control system",
  "type": "module",
  "main": "dist/main/electron/main.js",
  "scripts": {
    "start": "npm run build:electron && npm run build:preload && npm run build:react && electron .",
    "dev": "npm run build:electron && npm run build:preload && concurrently \"npm run dev:react\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "build:electron": "tsc -p tsconfig.electron.json && npm run copy:templates",
    "build:preload": "tsc -p tsconfig.preload.json",
    "copy:templates": "mkdir -p dist/main && cp -r templates dist/main/",
    "build:react": "webpack --config webpack.config.js --mode production",
    "dev:react": "webpack serve --config webpack.config.js --mode development",
    "dist": "npm run build:electron && npm run build:preload && npm run build:react && electron-builder",
    "clean": "rm -rf dist dist-electron dist-react",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "theater",
    "projection",
    "osc",
    "webrtc",
    "websockets",
    "qlab",
    "projection-mapping",
    "stage-control"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/yourusername/cuepernova#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/cuepernova.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/cuepernova/issues"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "electron": "^37.4.0",
    "express": "^4.18.2",
    "jquery": "^3.7.1",
    "maptastic": "^3.0.0",
    "morgan": "^1.10.0",
    "node-forge": "^1.3.1",
    "osc": "^2.4.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "ws": "^8.14.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@electron/notarize": "^2.5.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@types/express": "^5.0.3",
    "@types/jquery": "^3.5.33",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/node-forge": "^1.3.14",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^13.0.1",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron-builder": "^26.0.12",
    "html-webpack-plugin": "^5.6.4",
    "react-refresh": "^0.17.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "wait-on": "^8.0.4",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "build": {
    "appId": "com.cuepernova.app",
    "productName": "Cuepernova",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "static/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "dmg",
        "zip"
      ],
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "notarize": false
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "afterSign": "scripts/notarize.js"
  }
}
